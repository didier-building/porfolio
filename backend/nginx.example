server {
    listen 80;
    server_name example.com;

    location /static/ {
        alias /var/www/portfolio/backend/portfolio_backend/static/;
    }

    location /media/ {
        alias /var/www/portfolio/backend/portfolio_backend/media/;
    }

    location / {
        proxy_pass http://127.0.0.1:8000;
        include proxy_params;
    }
}
